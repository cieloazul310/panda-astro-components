{
  "name": "@my-monorepo/some-component",
  "version": "0.0.1",
  "description": "",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    },
    "./panda.json": {
      "import": "./dist/panda.json",
      "require": "./dist/panda.json"
    }
  },
  "files": [
    "dist"
  ],
  "keywords": [],
  "author": "",
  "license": "ISC",
  "scripts": {
    "reset": "rimraf node_modules && rimraf dist",
    "build": "tsc && cp src/*.astro dist",
    "codegen": "panda codegen --clean",
    "ship": "panda ship src/**/*.astro --outfile dist/panda.json",
    "build-ship": "npm run build && npm run ship",
    "prepare": "npm run codegen && npm run build && npm run ship"
  },
  "dependencies": {
    "@my-monorepo/preset-base": "^0.0.1"
  },
  "devDependencies": {
    "@my-monorepo/tsconfig": "^0.0.1",
    "@pandacss/dev": "^0.15.2",
    "@pandacss/preset-panda": "^0.15.2",
    "@pandacss/types": "^0.15.2",
    "astro": "^3.1.1",
    "eslint": "^8.50.0",
    "eslint-config-custom": "^0.0.1"
  },
  "peerDependencies": {
    "@pandacss/dev": "^0.13.0",
    "astro": "^3.0.0"
  }
}