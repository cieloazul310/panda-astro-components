{
  "name": "@my-monorepo/button",
  "version": "0.0.1",
  "description": "",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    },
    "./preset": {
      "types": "./dist/preset.d.ts",
      "import": "./dist/preset.mjs",
      "require": "./dist/preset.js"
    },
    "./panda.json": {
      "import": "./dist/panda.json",
      "require": "./dist/panda.json"
    }
  },
  "typesVersions": {
    "*": {
      "preset": [
        "dist/preset"
      ]
    }
  },
  "files": [
    "dist"
  ],
  "keywords": [],
  "author": "",
  "license": "ISC",
  "scripts": {
    "clean": "rimraf node_modules && rimraf dist",
    "codegen": "panda --clean",
    "dev": "npm-run-all --parallel dev:preset dev:panda dev:component",
    "dev:panda": "panda --watch",
    "dev:preset": "npm run tsup -- --watch",
    "dev:component": "cp src/*.astro dist && tsc --watch",
    "build": "npm run codegen && npm run build-ship",
    "build:all": "npm run build:preset && npm run build:component",
    "build:component": "tsc && cp src/*.astro dist",
    "build:preset": "npm run tsup -- --clean",
    "tsup": "tsup src/preset.ts --format=esm,cjs --dts --external @pandacss/dev --external @my-monorepo/preset-base",
    "ship": "panda ship src/**/*.astro --outfile dist/panda.json",
    "build-ship": "npm run build:all && npm run ship"
  },
  "dependencies": {
    "@my-monorepo/preset-base": "^0.0.1"
  },
  "devDependencies": {
    "@my-monorepo/tsconfig": "^0.0.1",
    "@pandacss/dev": "^0.15.3",
    "astro": "^3.1.4",
    "eslint": "^8.50.0",
    "eslint-config-custom": "^0.0.1"
  },
  "peerDependencies": {
    "@pandacss/dev": "^0.13.0",
    "astro": "^3.0.0"
  }
}