{
  "name": "@my-monorepo/button",
  "version": "0.0.1",
  "description": "",
  "type": "module",
  "main": "./src/index.ts",
  "types": "./src/index.ts",
  "exports": {
    ".": {
      "types": "./src/index.ts",
      "import": "./src/index.ts"
    },
    "./components": {
      "import": "./src/index.ts",
      "require": "./src/index.ts"
    },
    "./preset": {
      "types": "./dist/preset.d.ts",
      "import": "./dist/preset.js",
      "require": "./dist/preset.cjs"
    }
  },
  "typesVersions": {
    "*": {
      "preset": [
        "dist/preset"
      ]
    }
  },
  "files": [
    "dist",
    "src/index.ts",
    "src/*.astro"
  ],
  "keywords": [],
  "author": "",
  "license": "ISC",
  "scripts": {
    "clean": "rimraf node_modules && rimraf dist",
    "codegen": "panda --clean",
    "dev": "npm-run-all --parallel dev:preset dev:panda",
    "dev:panda": "panda --watch",
    "dev:preset": "npm run tsup -- --watch",
    "build": "npm run codegen && npm run build:preset",
    "build:preset": "npm run tsup -- --clean",
    "tsup": "tsup src/preset.ts --format=esm,cjs --dts --external @pandacss/dev --external @my-monorepo/preset-base"
  },
  "dependencies": {
    "@my-monorepo/preset-base": "^0.0.1"
  },
  "devDependencies": {
    "@my-monorepo/tsconfig": "^0.0.1",
    "@pandacss/dev": "^0.15.3",
    "astro": "^3.1.4",
    "eslint": "^8.50.0",
    "eslint-config-custom": "^0.0.1"
  },
  "peerDependencies": {
    "@pandacss/dev": "^0.13.0",
    "astro": "^3.0.0"
  }
}