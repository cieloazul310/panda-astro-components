{
  "name": "@my-monorepo/button",
  "version": "0.0.1",
  "description": "",
  "type": "module",
  "main": "./src/index.ts",
  "types": "./src/index.ts",
  "exports": {
    ".": {
      "types": "./src/index.ts",
      "import": "./src/index.ts",
      "require": "./src/index.ts"
    },
    "./preset": {
      "types": "./dist/preset.d.ts",
      "import": "./dist/preset.js",
      "require": "./dist/preset.cjs"
    }
  },
  "files": [
    "src",
    "dist",
    "index.ts"
  ],
  "keywords": [],
  "author": "",
  "license": "ISC",
  "scripts": {
    "reset": "rimraf node_modules && rimraf dist",
    "codegen": "panda codegen --clean",
    "build": "tsup src/preset.ts --format=esm,cjs --clean --dts --external @my-monorepo/ui-lib",
    "dev": "npm build --watch",
    "ship": "panda ship src/**/*.astro --outfile dist/panda.json",
    "build-ship": "npm run build && npm run ship",
    "prepare": "npm run codegen && npm run build-ship"
  },
  "dependencies": {
    "@my-monorepo/preset": "^0.0.1"
  },
  "devDependencies": {
    "@pandacss/dev": "^0.15.1",
    "@pandacss/preset-panda": "^0.15.1",
    "@pandacss/types": "^0.15.1",
    "astro": "^3.1.1"
  },
  "peerDependencies": {
    "@pandacss/dev": "^0.13.0",
    "astro": "^3.0.0"
  }
}