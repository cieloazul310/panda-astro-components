{
  "name": "@my-monorepo/button",
  "version": "0.0.1",
  "description": "",
  "main": "src/index.ts",
  "types": "src/index.ts",
  "exports": {
    ".": {
      "types": "./src/index.ts",
      "import": "./src/index.ts",
      "require": "./src/index.ts"
    },
    "./preset": {
      "types": "./src/preset.ts",
      "import": "./src/preset.ts",
      "require": "./src/preset.ts"
    }
  },
  "files": [
    "src"
  ],
  "keywords": [],
  "author": "",
  "license": "ISC",
  "scripts": {
    "reset": "rimraf node_modules && rimraf dist",
    "codegen": "panda codegen --clean",
    "build": "tsup src/index.ts src/preset.ts --dts --format=esm,cjs --clean --dts --external @pandacss/dev --external @my-monorepo/ui-lib",
    "dev": "npm build --watch",
    "preview": "vite",
    "ship": "panda ship src/**/*.astro --outfile src/panda.json",
    "build-ship": "npm build && npm ship"
  },
  "dependencies": {
    "@my-monorepo/preset": "^0.0.1"
  },
  "devDependencies": {
    "@pandacss/dev": "^0.15.1",
    "@pandacss/preset-panda": "^0.15.1",
    "@pandacss/types": "^0.15.1",
    "astro": "^3.1.1"
  },
  "peerDependencies": {
    "@pandacss/dev": "^0.13.0",
    "astro": "^3.0.0"
  }
}
