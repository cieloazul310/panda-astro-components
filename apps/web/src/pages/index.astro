---
import { css } from "@my-monorepo/ui-lib/css";
import { Button } from "@my-monorepo/button";
import { SomeComponent, UsePresetToken } from "@my-monorepo/some-component";
import Layout from "../layout/index.astro";
---
<Layout>
  <p class={css({ bg: "blue.100" })}>Hello, Astro!</p>
  <Button visual="edgy">Edgy</Button>
  <Button visual="funky">Funky</Button>
  <SomeComponent />
  <UsePresetToken>
    <p>Hi, bel canto.</p>
  </UsePresetToken>
</Layout>
